<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <navbar> 
             
                <p><a href="{{ path('app_home') }}">Hjem</a></p> 
                <p><a href="{{ path('app_temp') }}">Afdelinger</a></p>
                {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                <p><a href="{{ path('app_show_user_info') }}">Vis bruger info</a></p>
                <p><a href="{{ path('app_register') }}">Opret ny bruger</a></p>
                <p><a href="{{ path('admin') }}">Admin</a></p>
                <p><a href="{{ path('app_logout') }}">Logout</a></p>
                {% else %}
                    <p><a href="{{ path('app_login') }}">Login</a></p>
                {% endif %}
               

        </navbar> 
        {% block body %}{% endblock %}
    </body>
</html>
